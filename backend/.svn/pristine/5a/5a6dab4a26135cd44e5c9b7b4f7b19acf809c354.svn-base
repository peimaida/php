.container .titl1{
	width: 100%;
}

.container .titl1> h1 {
	font-size: 3.3rem;
	color: #0169c5;
	text-align: center;
	padding: 3rem 0;
	font-weight: 900;
}

.container  .content-show {
	width: 79.2%;
	margin: 0 auto;
	color: #333;
}

.container  .content-show> h4 {
	text-align: left;
	font-size: 1.6rem;
	padding-bottom: 1.5rem;
	font-weight: 700;
}

.container  .content-show> h5 {
	text-align: left;
	font-size: 1.6rem;
	padding-bottom: 1.5rem;
	font-weight: 500;
}

.container  .content-show .show-infor {
	width: 100%;
	padding-bottom: 4.5rem;
}

.container  .content-show .show-infor> p {
	width: 110%;
	text-align: left;
	padding-bottom: 0.7rem;
}

.container  .content-show .show-infor> p> strong {
	display: inline-block;
	width: 3rem;
	height: 3rem;
	background: url(../images/error/ico-1.png) no-repeat;
	background-size: 100%;
	text-align: center;
	line-height: 3rem;
	color: #ffffff;
	font-family: helvetica;
	font-weight: 800;
	font-size: 1.3rem;
	vertical-align: middle;
}

.container  .content-show .show-infor> p> span {
	font-size: 1.5rem;
	padding-left: 0.75rem;
	vertical-align: middle;
}

.container  .content-show .btn-content {
	width: 100%;
}

.container  .content-show .btn-content> ul {
	width: 100%;
	overflow: hidden;
	padding-bottom: 3rem;
}

.container  .content-show .btn-content> ul> li {
	width: 40%;	
}

.container  .content-show .btn-content> ul> li> a {
	display: block;
	width: 100%;
	height: 3rem;
	background: #0169c5;
	border-radius: 2px;
	text-align: center;
	text-decoration: none;
	line-height: 3rem;
	color: #ffffff;
	box-shadow: 0 2px 3px rgba(71, 71, 71, 0.31);
	-o-box-shadow: 0 2px 3px rgba(71, 71, 71, 0.31);
	-webkit-box-shadow: 0 2px 3px rgba(71, 71, 71, 0.31);
	-ms-box-shadow: 0 2px 3px rgba(71, 71, 71, 0.31);
}

.container  .content-show .btn-content> ul> li> a> span.two {
	display: inline-block;
	font-size: 1.4rem;
	background: url(../images/error/ico-2.png) no-repeat left center;
	background-size: 1.5rem;
	padding-left: 1.9rem;
}

.container  .content-show .btn-content> ul> li> a> span.one {
	display: inline-block;
	font-size: 1.4rem;
	background: url(../images/error/ico-3.png) no-repeat left center;
	background-size: 1.7rem;
	padding-left: 1.9rem;
}

.container  .content-show .btn-content> ul .left {
	float: left;
}

.container  .content-show .btn-content> ul .right {
	float: right;
}


