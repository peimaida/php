html{min-width: 320px;width: 100%;position: relative;}
*{margin: 0;padding: 0;font-family: "微软雅黑",Helvetica;}
ul,li{ list-style:none;}
img{ border:none;}
a{text-decoration: none;color: #000;}


html{font-size:10px;}
@media screen and (min-width:321px) and (max-width:375px){html{font-size:11px}}
@media screen and (min-width:376px) and (max-width:414px){html{font-size:12px}}
@media screen and (min-width:415px) and (max-width:639px){html{font-size:15px}}
@media screen and (min-width:640px) and (max-width:719px){html{font-size:20px}}
@media screen and (min-width:720px) and (max-width:749px){html{font-size:22.5px}}
@media screen and (min-width:750px) and (max-width:799px){html{font-size:23.5px}}
@media screen and (min-width:800px){html{font-size:25px}}


/*页头标题*/
.page-title{border-bottom: 1px #e9e9e9 solid;text-align: center;font-size: 1.7rem;color: #333;padding: .9rem 0;font-weight: normal;}
.page-title .page-title-img{display: inline-block;width: 1.8rem; margin-bottom: -.2rem; margin-right: .3rem;}

.container{padding-top: 5rem;min-height: 500px;}

/* header */
/* base-css */

ul,
ol {list-style: none;
}
iframe,
a,
img,
button {
  border: 0;
}
table {
  border-collapse: collapse;
  border-spacing: 0;
}
article,
aside,
footer,
header,
hgroup,
nav,
section,
figure,
figcaption {
  display: block;
}
a,
a:hover {
  text-decoration: none;
}
.clear {
  clear: both;
}

.clearfix:before,
.clearfix:after {
  content: "";
  display: table;
}
.clearfix:after {
  clear: both;
  overflow: hidden;
}
.hidden {
  display: none;
}
.t-left {
  text-align: left;
}
.t-center {
  text-align: center;
}
.t-right {
  text-align: right;
}
em,
i,
s {
  font-style: normal;
}
body {
  word-wrap: break-word;
  -webkit-text-size-adjust: none;
}

.display-flex {
  display: -webkit-flex;
  display: flex;
}

a {
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  /*苹果手持设备的浏览器属性，意思是：点击它，它就会出现一个半透明的灰色背景。webkit是苹果浏览器引擎，tap点击，highlight背景高亮，color颜色，颜色用数值调节。 */
}
/* base-css */



/* header_start */
header{
	position: fixed;
	background: #fff none repeat scroll 0 0;
    border-bottom: 1px solid #ccc;
    color: #111;
    padding: 0.8rem 0;
    width: 100%;
    height: 100%;
	box-sizing: border-box;
	display: inline-block;
    -webkit-box-pack: justify;
	justify-content: space-between;
    -webkit-box-align: center;
	height:5rem;
	z-index:99;
}

header .left{
	width:15%;
	float:left;
	justify-content: flex-start;
    -webkit-box-align: center;
	line-height:5rem;
	display:inline-block;
}

header .left img{
	padding-left: 1.125rem;
	text-align:center;
	display:inline;
	height:2.4rem;
}

header .center_item{
	float:left;
	width:70%;
	text-align: center;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
	line-height:5rem;
}

header .center_item img{
	display:inline;
	text-align:center;
	height: 3.1rem;
}

header .right{
	float:right;
	justify-content: flex-start;
    -webkit-box-align: center;
	line-height:5rem;
}

header .right img{
	padding-right: 1.125rem;
	display:inline;
	text-align:center;
	height:2.4rem;
}
/* header_end */


/* menu-list */
div.slide-mask {
  position:fixed;
  left: 0;
  top: 0;
  width: 100%;
  background:rgba(0,0,0,0.4);
  -webkit-transition: all 0.3s ease-out;
  -moz-transition: all 0.3s ease-out;
  -ms-transition: all 0.3s ease-out;
  -o-transition: all 0.3s ease-out;
  transition: all 0.3s ease-out;
  display: none;
  z-index:100;
}
aside.slide-wrapper {
  position: fixed;
  z-index: 4;
  left: 0;
  top: 0;
  width: 70%;
  background-color:#ffffff;
  -webkit-transform: translate3d(-100%, 0, 0);
  -moz-transform: translate3d(-100%, 0, 0);
  -ms-transform: translate3d(-100%, 0, 0);
  -o-transform: translate3d(-100%, 0, 0);
  transform: translate3d(-100%, 0, 0);
  -webkit-transition: all 0.3s ease-out;
  -moz-transition: all 0.3s ease-out;
  -ms-transition: all 0.3s ease-out;
  -o-transition: all 0.3s ease-out;
  transition: all 0.3s ease-out;
  overflow-y:auto;
  overflow-x:hidden;
  z-index:101;
}
aside.slide-wrapper > div {
  background-color: #ffffff;
  min-height: 100%;
  box-sizing: border-box;
  display:list-item;
  -webkit-box-pack: justify;
  justify-content: space-between;
  -webkit-box-align: center;
}

aside.slide-wrapper .slide-1st{
	padding:0;
	margin:0;
	test-align:center;
	width:100%;
}

aside.slide-wrapper.moved {
  -webkit-transform: translate3d(0, 0, 0);
  -moz-transform: translate3d(0, 0, 0);
  -ms-transform: translate3d(0, 0, 0);
  -o-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
}

aside.slide-wrapper li.current > a {
  opacity:0.8;
}

aside.slide-wrapper .slide-1st > a{
	width:100%;
	text-align:center;
	padding:0;
	margin:0;
	border-bottom: 1px solid #999;
	display: block;
    color: #000;
	display:inline-block;
}

aside.slide-wrapper .slide-1st img{
   width: 100%;
   display:inline;
   text-align:center;
   padding:0.3rem 0;
}

aside.slide-wrapper .slide-list{
  font-size: 18px;
  display: block;
  color: #000;
  width:100%;
  text-align:left;
  height:4.65rem;
  border-bottom: 1px solid #999;
  display:inline-block;
}

aside.slide-wrapper .slide-list .list-left{
	width:100%;
	margin-left:3%;
	overflow:hidden;
	display:inline-block;
	float:left;
	height:4.65rem;
	line-height:4.65rem;
}

aside.slide-wrapper .slide-list .list-left img{
	text-align:left;
	display:inline-block;
	height:2.3rem;
	margin-bottom:-0.35rem;
}

aside.slide-wrapper .slide-list .list-left span{
	text-align:left;
	padding-left:0.3rem;
	display:inline-block;
	font-size:1.6rem;
	color:#000;
	font-weight:999;
	
}

aside.slide-wrapper .slide-list .list-left .list-right{
	float:right;
	margin-right:8%;
	line-height:4.65rem;
	text-align:right;
	overflow:hidden;
	color:#016ac6;
	height:4.65rem;
	display:inline-block;
	text-align:right;
}

/* menu-list */


/* contact */
div.slide-contact {
  position:fixed;
  float:right;
  right: 0;
  top: 0;
  width: 100%;
  background:rgba(0,0,0,0.4);
  -webkit-transition: all 0.3s ease-out;
  -moz-transition: all 0.3s ease-out;
  -ms-transition: all 0.3s ease-out;
  -o-transition: all 0.3s ease-out;
  transition: all 0.3s ease-out;
  display: none;
  z-index:100;
}
aside.con-wrapper {
  position:fixed;
  z-index: 4;
  right: 0;
  top: 0;
  width: 94%;
  background-color:#ffffff;
  border-radius: 2%;
  -webkit-transform: translate3d(-100%, 0, 0);
  -moz-transform: translate3d(-100%, 0, 0);
  -ms-transform: translate3d(-100%, 0, 0);
  -o-transform: translate3d(-100%, 0, 0);
  transform: translate3d(-100%, 0, 0);
  -webkit-transition: all 0.3s ease-out;
  -moz-transition: all 0.3s ease-out;
  -ms-transition: all 0.3s ease-out;
  -o-transition: all 0.3s ease-out;
  transition: all 0.3s ease-out;
   overflow-y:auto;
  overflow-x:hidden;
  z-index:101;
}
aside.con-wrapper > div {
  border-radius: 2%;
  background-color: #ffffff;
  min-height: 100%;
  box-sizing: border-box;
  display:list-item;
  -webkit-box-pack: justify;
  justify-content: space-between;
  -webkit-box-align: center;
  list-style:none;
  padding-bottom: 4%;
}

aside.con-wrapper > div:after{
	content: "";
    display: block;
    width: 0;
    height: 0;
    border-left: 10px solid transparent;
    border-right: 10px solid transparent;
	border-bottom: 11px solid #fff;
    position:absolute;
    top: -10px;
    right: 1.425rem;
	display:none;
	}

aside.con-wrapper.moved {
  -webkit-transform: translate3d(0, 0, 0);
  -moz-transform: translate3d(0, 0, 0);
  -ms-transform: translate3d(0, 0, 0);
  -o-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
}

/* contact */
aside.con-wrapper .sub-title{
	width: 90%;
    height: 3.1rem;
    margin: 0 auto;
    padding-top: 1.6rem;
    border-bottom: 0.1rem solid #a7a7a7;
    font-weight: 900;
}
aside.con-wrapper .sub-title>p{
	height: 2rem;
    border-left: 0.5rem solid #0169c6;
    font-size: 2rem;
    line-height: 2rem;
    padding-left: 0.625rem;
    font-weight: 600;
    color: #000;
}

aside.con-wrapper .close_btn{
	position: absolute;
    display: block;
    right: 1.3rem;
    cursor: pointer;
    top: 1.3rem;
}

aside.con-wrapper .close_btn img{
	height:2.6rem;
}

.call-numble{
	background:url(../images/contact/call-ico.png) no-repeat left top;
