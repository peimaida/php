.container .sub-title{
	width:94.666%;
	height: 2.6rem;
	margin: 1.5rem auto 0;
	border-bottom: 0.125rem solid #a7a7a7;
	font-weight: 900;
}
.container .sub-title>p{
	height: 2.1rem;
	border-left:0.5rem solid #0169c6;
	font-size:1.8rem;
	line-height: 2.1rem;
	padding-left:0.5rem;
	font-weight: 500;
	letter-spacing: 0.15rem;
}
.container .call-numble{
	width: 93.333%;
	margin:1.2rem auto;
	height: 3rem;
}
.container .call-numble>span{	
	background: url(../images/contact/call-ico.png) no-repeat left -0.01rem;	
	padding-left: 3.25rem;
	color: #0169C6;
	font-weight: 500;	
	background-size:3rem 3rem;
	display: block;
}
.container .call-numble>span a{
	font-family:helvetica;
	line-height: 3rem;
	font-size: 4rem;
	text-decoration: none;	
	color: #0169C6;
}
.container .call-time{
	width: 100%;
	margin-bottom: 1.6rem;
}
.container .call-time span{
	color: #dd0000;
	display: block;
	font-size: 1.3rem;
	line-height: 1.5rem;
	padding-left: 4.5rem;
}
.container .show-content{
	width:89.2%;
	margin: 0 auto;
	padding-bottom: 3.2rem;
}
.container .show-content .sub-title-infor{
	width: 100%;
	height: 2.5rem;
	border-bottom: 0.0625rem solid #67a5dd;	
	margin-bottom: 1.5rem;
}
.container .show-content .sub-title-infor span{
	font-weight: 500;
	border-left:0.5rem solid #67a5dd;
	font-size: 1.6rem;
	line-height: 2rem;
	padding-left: 0.5rem;
	letter-spacing: 0.15rem;
}
.container .show-content .world-infor-1{
	font-size: 1.2rem;
	line-height: 1.28rem;
	margin-bottom:1rem;
}
.container .show-content .world-infor-1 span{
	color:#016ac6;
	margin:-0.18rem 0.1rem 0;
}
.container .show-content .world-infor-1 strong{
	font-weight: 900;
	color: #016ac6;
	vertical-align: middle;
	margin:-0.18rem 0.1rem 0;
	font-size: 1.6rem;
	display: inline-block;
	line-height: 1.28rem;
}
.container .show-content .world-infor-1 img{
	width:1.25rem;
	height:1.25rem;
	display: inline-block;
	vertical-align:middle;
	margin:-0.18rem 0.1rem 0;
}
.container .show-content .show-infor{
	width: 100%;
}
.container .show-content .show-infor table tr td{
	height:4.25rem;
}
.container .show-content .show-infor table tr td.oneL{
	width: 3.25rem;
	background: #67a5dd;
}
.container .show-content .show-infor table tr td.oneL-2{
	background:#67a5dd;
	height:16.4rem;
}
.container .show-content .show-infor table tr td.oneL-2 img{
	width: 100%;
}
.container .show-content .show-infor table tr td.space{
	width:0.25rem;
}
.container .show-content .show-infor table tr td.space-h{
	width: 100%;
	height: 0.25rem;
}
.container .show-content .show-infor table tr td.oneTW{
	width:21.65rem;
	background: #efefef;
	padding-left: 1.5rem;
}
.container .show-content .show-infor table tr td.oneTW-2 span{
	line-height: 3rem;
}
.container .show-content .show-infor table tr td.oneTW-2 p{
	background: url(../images/contact/number-ico.png) no-repeat left 0;
	background-size:1.3rem 13.35rem;	
	font-size: 1.2rem;
	padding-left: 1.8rem;
	line-height: 1.52rem;
	margin-bottom:0.5rem;
}
.container .show-content .show-infor table tr td.oneTW-2 p.img-ico-2{
	background-position:left -1.84rem;
}
.container .show-content .show-infor table tr td.oneTW-2 p.img-ico-3{
	background-position:left -3.92rem;
}
.container .show-content .show-infor table tr td.oneTW-2 p.img-ico-4{
	background-position:left -5.92rem;
}
.container .show-content .show-infor table tr td.oneTW-2 p.img-ico-5{
	background-position:left -7.92rem;
}
.container .show-content .show-infor table tr td.oneTW-2 p.img-ico-6{
	background-position:left -9.92rem;
}
.container .show-content .show-infor table tr td.oneTW-2 p.img-ico-7{
	background-position:left -11.84rem;
}
.container .show-content .show-infor table tr td.oneTW>span{
	font-size: 1.4rem;
}
.container .show-content .show-infor table tr td.oneTh{
	width: 6.65rem;
	background: #efefef;
}
.container .show-content .show-infor table tr td.oneTh>span{
	font-size: 1.3rem;
	display: inline-block;
	line-height: 1.3rem;
}
.container .show-content .show-infor table tr td.oneTh strong{
	margin:0 0.1875rem ;
	font-size: 2rem;
	color: #016ac6;
	font-family: helvetica;
	display: inline-block;
	line-height: 1.3rem;
	vertical-align: bottom
}

.container .show-content .show-infor table tr td.oneL>img{
	width: 100%;
}
@media only screen and (min-width: 100px) and (max-width: 540px) {
	.container .call-numble span a{
		font-size: 3.2rem;
	}
}
